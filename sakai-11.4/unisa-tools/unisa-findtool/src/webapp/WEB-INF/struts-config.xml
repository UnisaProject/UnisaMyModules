<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	<data-sources />
	<form-beans>
	</form-beans>

	<global-exceptions />
	<global-forwards />
	
	<action-mappings>
		<action
			parameter="action"
			path="/default"
			type="za.ac.unisa.lms.tools.findtool.actions.DefaultAction">
			<forward
				name="default"
				path="/WEB-INF/jsp/default.jsp"
				contextRelative="true" />
		</action>
	</action-mappings>

	<message-resources
		parameter="za.ac.unisa.lms.tools.findtool.ApplicationResources" />

	
</struts-config>